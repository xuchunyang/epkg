<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Emacs Packages</title>
    <link href="index.css" rel="stylesheet">
  </head>
  <body>
    <table>
      <thead>
        <th>Package</th>
        <th>Description</th>
      </thead>
      <tbody>
        {{#pkgs}}
        <tr>
          <th><a href="{{{name}}}">{{name}}</a></th>
          <td>{{summary}}</td>
        </tr>
        {{/pkgs}}
      </tbody>
    </table>
  </body>
</html>
